<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white4"
    android:orientation="vertical" >

    <include layout="@layout/af_main_title" />

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:fillViewport="true"
        android:scrollbars="none" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="15dp"
            android:background="@color/white4"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="15dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="10dp"
                android:background="@color/white4"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/background"
                    android:orientation="vertical" >

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/background"
                        android:orientation="horizontal"
                        android:padding="10dp" >

                        <TextView
                            android:id="@+id/af_logo_11_1"
                            style="@style/TextViewStyle"
                            android:layout_width="110dp"
                            android:layout_gravity="center_vertical"
                            android:text="@string/af_logo_11_1" />

                        <EditText
                            android:id="@+id/af_logo_11_unit1"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_weight="1"
                            android:background="@drawable/pwd_background"
                            android:hint="@string/af_logo_11_unit"
                            android:inputType="number"
                            android:padding="5dp" />
                    </LinearLayout>

                    <ImageView
                        android:layout_width="fill_parent"
                        android:layout_height="0.5dp"
                        android:background="@color/black4"
                        android:contentDescription="@string/action_settings" />

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:orientation="horizontal"
                        android:padding="10dp" >

                        <TextView
                            android:id="@+id/af_logo_11_2"
                            style="@style/TextViewStyle"
                            android:layout_width="110dp"
                            android:layout_gravity="center_vertical"
                            android:text="@string/af_logo_11_2" />

                        <EditText
                            android:id="@+id/af_logo_11_unit2"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_weight="1"
                            android:background="@drawable/pwd_background"
                            android:hint="@string/af_logo_11_unit"
                            android:inputType="number"
                            android:padding="5dp" />
                    </LinearLayout>

                    <ImageView
                        android:layout_width="fill_parent"
                        android:layout_height="0.5dp"
                        android:background="@color/black4"
                        android:contentDescription="@string/action_settings" />

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:orientation="horizontal"
                        android:padding="10dp" >

                        <TextView
                            android:id="@+id/af_logo_11_3"
                            style="@style/TextViewStyle"
                            android:layout_width="110dp"
                            android:layout_gravity="center_vertical"
                            android:text="@string/af_logo_11_3" />

                        <EditText
                            android:id="@+id/af_logo_11_unit3"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_weight="1"
                            android:background="@drawable/pwd_background"
                            android:hint="@string/af_logo_11_unit"
                            android:inputType="number"
                            android:padding="5dp" />
                    </LinearLayout>

                    <ImageView
                        android:layout_width="fill_parent"
                        android:layout_height="0.5dp"
                        android:background="@color/black4"
                        android:contentDescription="@string/action_settings" />

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:orientation="horizontal"
                        android:padding="10dp" >

                        <TextView
                            android:id="@+id/af_logo_11_4"
                            style="@style/TextViewStyle"
                            android:layout_width="110dp"
                            android:layout_gravity="center_vertical"
                            android:text="@string/af_logo_11_4" />

                        <EditText
                            android:id="@+id/af_logo_11_unit4"
