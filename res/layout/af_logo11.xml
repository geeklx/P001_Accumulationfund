                            android:layout_width="110dp"
                            android:layout_gravity="center_vertical"
                            android:text="@string/af_logo_11_8" />

                        <EditText
                            android:id="@+id/af_logo_11_unit7"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_weight="1"
                            android:background="@drawable/pwd_background"
                            android:hint="@string/af_logo_11_unit"
                            android:inputType="number"
                            android:padding="5dp" />
                    </LinearLayout>

                    <ImageView
                        android:layout_width="fill_parent"
                        android:layout_height="0.5dp"
                        android:background="@color/black4"
                        android:contentDescription="@string/action_settings" />

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:orientation="horizontal"
                        android:padding="10dp" >

                        <TextView
                            android:id="@+id/af_logo_11_9"
                            style="@style/TextViewStyle"
                            android:layout_width="110dp"
                            android:layout_gravity="center_vertical"
                            android:text="@string/af_logo_11_9" />

                        <EditText
                            android:id="@+id/af_logo_11_unit8"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_weight="1"
                            android:background="@drawable/pwd_background"
                            android:hint="@string/af_logo_11_unit"
                            android:inputType="number"
                            android:padding="5dp" />
                    </LinearLayout>

                    <ImageView
                        android:layout_width="fill_parent"
                        android:layout_height="0.5dp"
                        android:background="@color/black4"
                        android:contentDescription="@string/action_settings" />

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/background"
                        android:orientation="horizontal"
                        android:padding="10dp" >

                        <TextView
                            android:id="@+id/af_logo_11_10"
                            style="@style/TextViewStyle"
                            android:layout_width="110dp"
                            android:layout_gravity="center_vertical"
                            android:text="@string/af_logo_11_10" />

                        <RadioGroup
                            android:id="@+id/rg_11"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="5dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="1"
                            android:background="@color/white"
                            android:gravity="center"
                            android:orientation="horizontal" >

                            <RadioButton
                                android:id="@+id/rb_11"
                                style="@style/RadioButtonStyles"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="@string/af_logo_11_man"
                                android:textColor="@android:color/black" />

                            <RadioButton
                                android:id="@+id/rb_22"
                                style="@style/RadioButtonStyles"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:text="@string/af_logo_11_female"
                                android:textColor="@android:color/black" />
                        </RadioGroup>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <Button
                android:id="@+id/btn_msg"
                android:layout_width="110dp"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                android:layout_margin="10dp"
                android:background="@drawable/btn_able"
                android:padding="15dp"
                android:text="@string/af_logo_10_calculate"
                android:textColor="@color/white"
                android:textSize="16sp" />
        </LinearLayout>
    </ScrollView>

</LinearLayout>